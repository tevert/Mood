@model Mood.ViewModels.ResultsViewModel
@{
    ViewBag.Title = Model.Survey.Description;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section SPAViews {
    @Html.Partial("_Answers", Model)
}
@section Scripts{
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/util")
    @Scripts.Render("~/bundles/chartjs")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/resultsApp")
}